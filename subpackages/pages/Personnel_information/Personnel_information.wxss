/* subpackages/pages/Personnel_information/Personnel_information.wxss */
page{
  height: 100%;
  margin: 0;
  padding: 0;
 background-color: #F0EFF4;/* 设置你需要的背景色 */
}
.box{
  width: 100%;
		min-height: 100vh;
		  display: flex;
		  flex-direction: column;
		background-color: #F0EFF4;
}
.box_list{
  width: 702rpx;
  background: #FFFFFF;
  border-radius: 20rpx 20rpx 20rpx 20rpx;
margin-left: 50%;
transform: translateX(-50%);
padding: 34rpx 28rpx;
margin-bottom: 32rpx;
}
.box_list view:not(:first-child){
  margin-top: 34rpx;
  font-family: PingFang SC, PingFang SC;
font-weight: 400;
font-size: 28rpx;
color: #0B1944;
}
#btn{
  width: 90%;
height: 100rpx;
line-height: 100rpx;
background: #FF8F1F;
font-family: PingFang SC, PingFang SC;
font-weight: 400;
font-size: 36rpx;
color: #FFFFFF;
text-align: center;
border-radius: 24rpx 24rpx 24rpx 24rpx;
margin-left: 50%;
transform: translateX(-50%);
padding: 0rpx !important;
}
.box_info{
  margin-top: 50%;
  margin-left: 50%;
  transform: translate(-29%,-50%);
  font-size: 32rpx;
  color: red;
}