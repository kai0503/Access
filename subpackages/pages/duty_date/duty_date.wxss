
.table {
  border: 0px solid darkgray;
  background-color: #fff;
}
.tr {
  display: flex;
  width: 100%;
  justify-content: center;
  height: 100rpx;
  font-size: 25rpx;
}
.td {
    width:40%;
    margin-top: 1rpx;
     justify-content: center;
     align-items: center;
     padding: 20rpx 0rpx;
     display: flex;
    text-align: center;
    word-wrap: break-word;
}
.bg-w{
  background: snow;
  line-height: 100rpx;
}
.bg-g{
  background: #E6F3F9;
  margin-bottom: 20rpx;
}
.th {
  width: 40%;
  justify-content: center;
  background: #3366FF;
  color: #fff;
  display: flex;
  height: 100rpx;
  font-size: 33rpx;
}
