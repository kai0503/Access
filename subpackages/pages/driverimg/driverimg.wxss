.box{
  width: 100%;
  height: 100vh;
  background-color: #F0EFF4;
}
.box_title{
  width: 100%;
  height: 100rpx;
  line-height: 100rpx;
  background-color: #FFFFFF;
  font-size: 32rpx;
  font-family: PingFang SC-Bold, PingFang SC;
  font-weight: bolder;
  color: #303347;
  padding-left: 24rpx;
  
}
.box_title image{
  width: 30rpx;
  height: 30rpx;
  position: relative;
  top: 25rpx;
  right: 59rpx;
}


.box_img{
  width: 750rpx;
  height: 446rpx;
  background: #FFFFFF;
  box-shadow: 0rpx 2rpx 0rpx 0rpx #E6E6E6;
  border-radius: 0rpx 0rpx 0rpx 0rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  opacity: 1;
  margin-top: 24rpx;
  
  text-align: center;
  
}
.img_chong{
    
  width: 492rpx;
			height: 266rpx;
      display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  background: #F7F7F7;
  border-radius: 24rpx 24rpx 24rpx 24rpx;
  opacity: 1;
  border: 2rpx dashed rgba(48,51,71,0.5);
  font-size: 32rpx;
  font-family: PingFang SC-Medium, PingFang SC;
  font-weight: 400;
  color: #303347;
  
}
.img_chong image{
  width: 64rpx;
  height: 64rpx;
}
.pics{
  width: 492rpx;
  height: 266rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  
  border-radius: 24rpx 24rpx 24rpx 24rpx;
  opacity: 1;
  border: 2rpx solid rgba(48,51,71,0.5);
  
 
}
.pics  image{
  width: 100%;
  height: 100%;
    border-radius: 24rpx 24rpx 24rpx 24rpx;
}
.submit{
  width: 702rpx;
  height: 88rpx;
  background: #CDF41B;
  border-radius: 48rpx 48rpx 48rpx 48rpx;
  opacity: 1;

  font-size: 36rpx;
  font-family: PingFang SC-Bold, PingFang SC;
  font-weight: bold;
  color: #303347;
  line-height: 88rpx;
  text-align: center;
  margin-left: 50%;
  transform: translateX(-50%);
  position: fixed;
  bottom: 50rpx;
}