
.item{
  width: 100%;
  height: 100vh;
 
}
.box{
  width: 100%;
  height: 100%;
  z-index: 1;
  position: relative;
}
.box1{
  width: 500rpx;
  height:200rpx;
  position: absolute;
  z-index: 999;
  top: 20rpx;
  left: 50%;
  transform: translate(-45%);
  background-color: white;
  border-radius: 40rpx;
}
.box1 button{
  margin-top: 4%;
  transform: translate();
  width: 90%;
  height: 80%;
  font-size: 60rpx;
  line-height: 160rpx;
  background-color: #FFDF4F;
  border-radius: 40rpx;
  color: white;
}
